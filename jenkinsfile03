pipeline {
    agent none  
    stages {
        stage('Parallel build tests') {
         steps {
        	parallel (
	        			'parallel stage one' :{
				          
				                echo 'parallel stage one'  
			          }, 
			          'parallel stage two' :{
				         
				                echo 'parallel stage two'
				        } 
	          )
	         }
	        stage('Serial stage') {
            agent { label "ASICVLD02" } 
            	steps {
                echo 'serial stage'
               }
        	}
       }
		 		
    }
}
